{
  "extended_format": true,
  "dataset": {
    "extract_voice": true,
    "in_memory_percentage": 1,
    "noise_only_percent": 0.1,
    "num_condition_classes": 29,
    "path": "data/NSDTSEA/",
    "regain": 0.06,
    "sample_rate": 16000,
    "type": "nsdtsea"
  },
  "model": {
    "condition_encoding": "binary",
    "no_conditioning": true,
    "dilations": 9,
    "filters": {
      "lengths": {
        "res": 3,
        "final": [
          3,
          3
        ],
        "skip": 1
      },
      "depths": {
        "res": 128,
        "skip": 128,
        "final": [
          2048,
          256
        ]
      }
    },
    "num_stacks": 3,
    "target_field_length": 1601,
    "target_padding": 1,
    "dropout": {
      "use": false,
      "rates": {
        "res": 0.3,
        "final": 0.3,
        "skip": 0.3
      }
    },
    "wiener_filter": {
      "pre": false,
      "post": false
    }
  },
  "optimizer": {
    "decay": 0.0,
    "epsilon": 1e-08,
    "lr": 0.001,
    "momentum": 0.9,
    "type": "adam"
  },
  "training": {
    "batch_size": 10,
    "early_stopping_patience": 16,
    "loss": {
      "out_1": {
        "l1": 1,
        "l2": 0,
        "weight": 1,
        "spec_param":
        {
          "frame_len": 1024,
          "frame_step": 512,
          "nfft": 1024
        },
        "spectrogram": {
          "weight": 0
        },
        "spectral_convergence": {
          "weight": 0
        },
        "weighted_spectrogram": {
          "weight": 0,
          "center_frequency": 2000,
          "std": 500
        },
        "rms": {
          "weight": 0
        }
      },
      "out_2": {
        "l1": 1,
        "l2": 0,
        "weight": 1,
        "spec_param":
        {
          "frame_len": 1024,
          "frame_step": 512,
          "nfft": 1024
        },
        "spectrogram": {
          "weight": 0
        },
        "spectral_convergence": {
          "weight": 0
        },
        "weighted_spectrogram": {
          "weight": 0,
          "center_frequency": 2000,
          "std": 500
        },
        "rms": {
          "weight": 0
        }
      }
    },
    "num_epochs": 250,
    "num_test_samples": 100,
    "num_train_samples": 1000,
    "path": "sessions/default_5_new_config",
    "verbosity": 1
  }
}